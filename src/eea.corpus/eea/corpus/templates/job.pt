<div metal:use-macro="load: layout.pt">

  <div metal:fill-slot="sidebar">
  </div>

  <div metal:fill-slot="content">
    <div class="content" >
      <h4>Job id ${job.id}</h4>
      <p class="discreet">
      Created: ${job.created_at}<br/>
      </p>
      <p>
        <strong>Status</strong>
        ${job._status}
      </p>

      <h5>Arguments</h5>
      <pre>
      <code>
      ${job.kwargs}
      </code>
      </pre>

      <h5>Execution info</h5>
      <pre>
      <code>
      ${job.exc_info}
      </code>
      </pre>

    </div>
  </div>
</div>

